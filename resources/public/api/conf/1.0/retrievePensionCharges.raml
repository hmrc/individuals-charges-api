    body:
      application/json:
         type: !include schemas/retrieve_pension_charges_response.json
         examples:
           example-1:
            description: Retrieve Pension Charges
            value: !include examples/retrievePensionCharges/retrieve_pension_charges_response.json
    headers:
      X-CorrelationId:
        example: c75f40a6-a3df-4429-a697-471eeec46435
        description: Unique ID for operation tracking <br> String, 36 characters.
