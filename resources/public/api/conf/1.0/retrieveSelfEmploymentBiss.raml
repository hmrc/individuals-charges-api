is:
  - headers.acceptJson
  - headers.contentTypeJson
  - headers.testScenarioHeader
  - errors.formatNino
  - errors.formatTaxYear
  - errors.formatSelfEmploymentId
  - errors.ruleTaxYearRangeInvalid
